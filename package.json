{
	"name": "cypress-e2e-starter",
	"version": "1.0.0",
	"repository": "git@github.com:alexandrepasc/cypress-e2e-starter.git",
	"author": "Alexandre Pascoal <alexandre.pascoal@gmail.com>",
	"license": "MIT",
	"scripts": {
		"cy:open": "cypress open --e2e",
		"cy:open:chrome": "cypress open --e2e --browser chrome",
		"cy:open:firefox": "cypress open --e2e --browser firefox",
		"cy:run": "cypress run --e2e",
		"cy:run:chrome": "cypress run --e2e --browser chrome",
		"cy:run:firefox": "cypress run --e2e --browser firefox",
		"ex:run:chrome": "cypress run --e2e --browser chrome --config-file cypress.ex.config.ts --reporter-options 'reportFilename=\"[status]_[datetime]-e2e-chrome\"'",
		"ex:run:firefox": "cypress run --e2e --browser firefox --config-file cypress.ex.config.ts --reporter-options 'reportFilename=\"[status]_[datetime]-e2e-firefox\"'",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "echo \"[Husky] pre-commit\"",
			"commit-msg": "echo \"[Husky] commit-msg\""
		}
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@simonsmith/cypress-image-snapshot": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"cypress": "^13.7.0",
		"cypress-mochawesome-reporter": "^3.8.2",
		"eslint": "^8.57.0",
		"eslint-plugin-cypress": "^2.15.1",
		"husky": "^8.0.0",
		"mocha": "^10.3.0",
		"pg": "^8.11.3",
		"typescript": "^5.4.2"
	}
}
